		(210.0, 220.0, 230.0, 240.0)	-> (210.0, 220.0, 230.0, 240.0)
		(250.0, 260.0, 270.0, 280.0)	-> (250.0, 260.0, 270.0, 280.0)
		(290.0, 300.0, 310.0, 320.0)	-> (290.0, 300.0, 310.0, 320.0)
		(330.0, 340.0, 350.0, 360.0)	-> (290.0, 300.0, 310.0, 320.0)
		(370.0, 380.0, 390.0, 400.0)	-> (370.0, 380.0, 390.0, 400.0)
    -->
    
    <comp:spilloverMatrix comp:id="NoParams">

        <comp:spillover comp:parameter="ES">
            <comp:coefficient comp:parameter="HS" comp:value="0.55555"/>
            <comp:coefficient comp:parameter="GS" comp:value="0.66666"/>
            <comp:coefficient comp:parameter="FS" comp:value="0.77777"/>
        </comp:spillover>
        
        <comp:spillover comp:parameter="FS">
            <comp:coefficient comp:parameter="HS" comp:value="0.88888"/>
            <comp:coefficient comp:parameter="GS" comp:value="0.99999"/>
            <comp:coefficient comp:parameter="ES" comp:value="0.10101"/>
        </comp:spillover>
        
        <comp:spillover comp:parameter="GS">
            <comp:coefficient comp:parameter="ES" comp:value="0.33333"/>
            <comp:coefficient comp:parameter="FS" comp:value="0.11111"/>
            <comp:coefficient comp:parameter="HS" comp:value="0.12345"/>
        </comp:spillover>
 
        <comp:spillover comp:parameter="GS">
            <comp:coefficient comp:parameter="ES" comp:value="0.22222"/>
            <comp:coefficient comp:parameter="FS" comp:value="0.67890"/>
            <comp:coefficient comp:parameter="HS" comp:value="0.44444"/>
        </comp:spillover>
        
    </comp:spilloverMatrix>
    
    
</comp:Compensation-ML>
    
    