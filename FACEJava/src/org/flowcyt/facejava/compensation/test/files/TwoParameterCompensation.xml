<?xml version="1.0" encoding="UTF-8"?>

<comp:Compensation-ML 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:comp="http://www.isac-net.org/std/Compensation-ML/v1.0/"
    xsi:schemaLocation="http://www.isac-net.org/std/Compensation-ML/v1.0/ ../schema/Compensation-ML.v1.0.xsd">
    
    <!-- Let D be the set of parameters in the DataSet and S be the set of parameters in a
         spilloverMatrix.
         
         All the resulting compensators (when applied to the appropriate fcs data set should
         result in 2 parameters being compensated (except for the last matrix where we test
         that what happens when D {intersection} S = {empty set} (i.e., no compensation should
         be applied). 
    -->
    
    <!-- 
    	FCS File: int-10_events_2_parameters.fcs    	
     	
     	Case: S = D
     	
     	Compensation Matrix:
     	
     	   AS   BS
     	AS 0.95 0.15
     	BS 0.03 1.0
     	
     	Compensated Events:
     	(10.0, 20.0) 	-> (7.4034902168165, 19.7778952934955)
		(30.0, 40.0) 	-> (25.3833950290851, 39.2384981491274)
		(50.0, 60.0) 	-> (43.3632998413538, 58.6991010047594)
		(70.0, 80.0)	-> (61.3432046536224, 78.1597038603913)
		(90.0, 100.0) 	-> (79.3231094658911, 97.6203067160233)
		(110.0, 120.0) 	-> (97.3030142781597, 117.080909571655)
		(130.0, 140.0) 	-> (115.282919090428, 136.541512427287)
		(150.0, 160.0) 	-> (133.262823902697, 156.002115282919)
		(170.0, 180.0) 	-> (151.242728714966, 175.462718138551)
		(190.0, 200.0) 	-> (169.222633527234, 194.923320994183)
    -->
    
    <comp:spilloverMatrix comp:id="Equal">
      
        <comp:spillover comp:parameter="AS">
            <comp:coefficient comp:parameter="AS" comp:value="0.95"/>
            <comp:coefficient comp:parameter="BS" comp:value="0.15"/>
        </comp:spillover>
 
        <comp:spillover comp:parameter="BS">
            <comp:coefficient comp:parameter="AS" comp:value="0.03"/>
        </comp:spillover>
        
    </comp:spilloverMatrix>
    
    <!-- 
        FCS File: int-10_events_4_parameters.fcs    	
     	
     	Case: S {proper subset of} D
     	
     	Compensation Matrix:
     	
     	   AS   BS
     	AS 0.93 0.0
     	BS 0.23 1.0
     	
     	Compensated Events:
     	(10.0, 20.0, 30.0, 40.0)		-> (10.752688172043, 17.5268817204301, 30.0, 40.0)
		(50.0, 60.0, 70.0, 80.0)		-> (53.763440860215, 47.6344086021505, 70.0, 80.0)
		(90.0, 100.0, 110.0, 120.0)		-> (96.7741935483871, 77.741935483871, 110.0, 120.0)
		(130.0, 140.0, 150.0, 160.0)	-> (139.784946236559, 107.849462365591, 150.0, 160.0)
		(170.0, 180.0, 190.0, 200.0)	-> (182.795698924731, 137.956989247312, 190.0, 200.0)
		(210.0, 220.0, 230.0, 240.0)	-> (225.806451612903, 168.064516129032, 230.0, 240.0)
		(250.0, 260.0, 270.0, 280.0)	-> (268.817204301075, 198.172043010753, 270.0, 280.0)
		(290.0, 300.0, 310.0, 320.0)	-> (311.827956989247, 228.279569892473, 310.0, 320.0)
		(330.0, 340.0, 350.0, 360.0)	-> (354.838709677419, 258.387096774194, 350.0, 360.0)
		(370.0, 380.0, 390.0, 400.0)	-> (397.849462365591, 288.494623655914, 390.0, 400.0)
    -->
    
    <comp:spilloverMatrix comp:id="S-Sub-D">
        
        <comp:spillover comp:parameter="AS">
            <comp:coefficient comp:parameter="AS" comp:value="0.93"/>
        </comp:spillover>
 
        <comp:spillover comp:parameter="BS">
            <comp:coefficient comp:parameter="AS" comp:value="0.23"/>
        </comp:spillover>
        
    </comp:spilloverMatrix>
    
    <!-- 
        FCS File: int-10_events_2_parameters.fcs    	
     	
     	Case: D {proper subset of} S
     	
     	Compensation Matrix:
     	
     	   AS      BS
     	AS 1.0     0.16501
     	BS 0.01454 1.0
     	
     	Compensated Events:
     	(10.0, 20.0) 	-> (6.71591312366876, 19.9023506231819)
		(30.0, 40.0) 	-> (23.4558764035642, 39.6589515570922)
		(50.0, 60.0) 	-> (40.1958396834597, 59.4155524910025)
		(70.0, 80.0)	-> (56.9358029633551, 79.1721534249128)
		(90.0, 100.0) 	-> (73.6757662432506, 98.9287543588231)
		(110.0, 120.0) 	-> (90.415729523146, 118.685355292733)
		(130.0, 140.0) 	-> (107.155692803042, 138.441956226644)
		(150.0, 160.0) 	-> (123.895656082937, 158.198557160554)
		(170.0, 180.0) 	-> (140.635619362832, 177.955158094464)
		(190.0, 200.0) 	-> (157.375582642728, 197.711759028375)
    -->
    
    <comp:spilloverMatrix comp:id="D-Sub-S">
        
        <comp:spillover comp:parameter="AS">
            <comp:coefficient comp:parameter="BS" comp:value="0.16501"/>
            <comp:coefficient comp:parameter="CS" comp:value="0.33333"/>
            <comp:coefficient comp:parameter="DS" comp:value="0.11111"/>
        </comp:spillover>
 
        <comp:spillover comp:parameter="BS">
            <comp:coefficient comp:parameter="AS" comp:value="0.01454"/>
            <comp:coefficient comp:parameter="CS" comp:value="0.22222"/>
            <comp:coefficient comp:parameter="DS" comp:value="0.44444"/>
        </comp:spillover>
        
        <comp:spillover comp:parameter="CS">
            <comp:coefficient comp:parameter="AS" comp:value="0.55555"/>
            <comp:coefficient comp:parameter="BS" comp:value="0.66666"/>
            <comp:coefficient comp:parameter="DS" comp:value="0.77777"/>
        </comp:spillover>
        
        <comp:spillover comp:parameter="DS">
            <comp:coefficient comp:parameter="AS" comp:value="0.88888"/>
            <comp:coefficient comp:parameter="BS" comp:value="0.99999"/>
            <comp:coefficient comp:parameter="CS" comp:value="0.10101"/>
        </comp:spillover>
        
    </comp:spilloverMatrix>
    
    <!-- 
        FCS File: int-10_events_4_parameters.fcs    	
     	
     	Case: D {intersection} S != {empty set} and 
     	      D {not subset of} S and
     	      S {not subset of} D and
     	      |D {intersection} S| = 2
     	
     	Compensation Matrix:
     	
     	   BS      DS
     	BS 1.0     0.67890
     	DS 0.12345 1.0
     	
     	Compensated Events:
     	(10.0, 20.0, 30.0, 40.0)		-> (10.0, -7.81060871781485, 30.0, 40.9642196462142)
		(50.0, 60.0, 70.0, 80.0)		-> (50.0, 6.20832062422176, 70.0, 79.2335828189398)
		(90.0, 100.0, 110.0, 120.0)		-> (90.0, 20.2272499662584, 110.0, 117.502945991665)
		(130.0, 140.0, 150.0, 160.0)	-> (130.0, 34.246179308295, 150.0, 155.772309164391)
		(170.0, 180.0, 190.0, 200.0)	-> (170.0, 48.2651086503316, 190.0, 194.041672337117)
		(210.0, 220.0, 230.0, 240.0)	-> (210.0, 62.2840379923682, 230.0, 232.311035509842)
		(250.0, 260.0, 270.0, 280.0)	-> (250.0, 76.3029673344047, 270.0, 270.580398682568)
		(290.0, 300.0, 310.0, 320.0)	-> (290.0, 90.3218966764414, 310.0, 308.849761855293)
		(330.0, 340.0, 350.0, 360.0)	-> (330.0, 104.340826018478, 350.0, 347.119125028019)
		(370.0, 380.0, 390.0, 400.0)	-> (370.0, 118.359755360515, 390.0, 385.388488200744)
    -->
    
    <comp:spilloverMatrix comp:id="D-Int-S">

        <comp:spillover comp:parameter="ES">
            <comp:coefficient comp:parameter="DS" comp:value="0.55555"/>
            <comp:coefficient comp:parameter="BS" comp:value="0.66666"/>
            <comp:coefficient comp:parameter="FS" comp:value="0.77777"/>
        </comp:spillover>
        
        <comp:spillover comp:parameter="FS">
            <comp:coefficient comp:parameter="DS" comp:value="0.88888"/>
            <comp:coefficient comp:parameter="BS" comp:value="0.99999"/>
            <comp:coefficient comp:parameter="ES" comp:value="0.10101"/>
        </comp:spillover>
        
        <comp:spillover comp:parameter="DS">
            <comp:coefficient comp:parameter="ES" comp:value="0.33333"/>
            <comp:coefficient comp:parameter="FS" comp:value="0.11111"/>
            <comp:coefficient comp:parameter="BS" comp:value="0.12345"/>
        </comp:spillover>
 
        <comp:spillover comp:parameter="BS">
            <comp:coefficient comp:parameter="ES" comp:value="0.22222"/>
            <comp:coefficient comp:parameter="DS" comp:value="0.67890"/>
            <comp:coefficient comp:parameter="FS" comp:value="0.44444"/>
        </comp:spillover>
        
    </comp:spilloverMatrix>
    
    <!-- 
        FCS File: int-10_events_4_parameters.fcs    	
     	
     	Case: D {intersection} S = {empty set} 
     	
     	Compensation Matrix:
     	
     	Empty
     	
     	Compensated Events:
     	(10.0, 20.0, 30.0, 40.0)		-> (10.0, 20.0, 30.0, 40.0)
		(50.0, 60.0, 70.0, 80.0)		-> (50.0, 60.0, 70.0, 80.0)
