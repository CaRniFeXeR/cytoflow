<?xml version="1.0" encoding="UTF-8"?>

<gating:Gating-ML 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gating="http://www.isac-net.org/std/Gating-ML/v1.1/"
    xsi:schemaLocation="http://www.isac-net.org/std/Gating-ML/v1.1/ ../schema/Gating-ML.v1.1.xsd" >
  
    <gating:PolygonGate gating:id="R1">
        <gating:dimension gating:parameter="linToLog P5" />
        <gating:dimension gating:parameter="SSC-H" />
        <gating:vertex>
        	<gating:coordinate gating:value="2.2578124999999996" />
        	<gating:coordinate gating:value="242.0" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="2.2578124999999996" />
        	<gating:coordinate gating:value="39.0" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="2.55078125" />
        	<gating:coordinate gating:value="7.0" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="3.1484374999999996" />
        	<gating:coordinate gating:value="8.0" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="3.1484374999999996" />
        	<gating:coordinate gating:value="238.0" />
        </gating:vertex>
    </gating:PolygonGate>
    
 	<gating:RectangleGate gating:id="M1a">
        <gating:dimension gating:parameter="linToLog P3" gating:min="0" gating:max="1.2148437499999998" />
    </gating:RectangleGate>
    
    <gating:RectangleGate gating:id="M2a">
        <gating:dimension gating:parameter="linToLog P3" gating:min="1.2148437499999998" gating:max="3.9960937499999996" />
    </gating:RectangleGate>
    
    <!-- Equivalent to the Polytope gates but as Rectangles.-->
    <!--
    <gating:RectangleGate gating:id="M1b">
        <gating:dimension gating:parameter="linToLog P4" gating:min="0" gating:max="1.3203124999999998" />
    </gating:RectangleGate>
    
    <gating:RectangleGate gating:id="M2b">
        <gating:dimension gating:parameter="linToLog P4" gating:min="1.3203124999999998" gating:max="3.9960937499999996" />
    </gating:RectangleGate>
    -->
    
    <gating:PolytopeGate gating:id="M1b">
        <gating:dimension gating:parameter="linToLog P4" />
        <gating:point>
            <gating:coordinate gating:value="0" />
        </gating:point>
        <gating:point>
            <gating:coordinate gating:value="1.3203124999999998" />
        </gating:point>
    </gating:PolytopeGate>
    
    <gating:PolytopeGate gating:id="M2b">
        <gating:dimension gating:parameter="linToLog P4" />
        <gating:point>
            <gating:coordinate gating:value="3.9960937499999996" />
        </gating:point>
        <gating:point>
            <gating:coordinate gating:value="1.3203124999999998" />
        </gating:point>
    </gating:PolytopeGate>
    
    
    <gating:RectangleGate gating:id="Q1.UL">
        <gating:dimension gating:parameter="linToLog P3" gating:max="1.19140625"  />
        <gating:dimension gating:parameter="linToLog P4" gating:min="1.3046874999999998" />
    </gating:RectangleGate>
    
    <gating:DecisionTreeGate gating:id="Q1.UR">
        <gating:rootNode gating:parameter="linToLog P3" gating:threshold="1.19140625">
            <gating:leafLT gating:inside="false"/>
             <gating:nodeGTE gating:parameter="linToLog P4" gating:threshold="1.3046874999999998">
                <gating:leafLT gating:inside="false"/>
                <gating:leafGTE gating:inside="true"/>
            </gating:nodeGTE>
        </gating:rootNode>
    </gating:DecisionTreeGate>
    
    <gating:RectangleGate gating:id="Q1.LL">
        <gating:dimension gating:parameter="linToLog P3" gating:max="1.19140625"  />
        <gating:dimension gating:parameter="linToLog P4" gating:max="1.3046874999999998" />
    </gating:RectangleGate>
    
    <gating:DecisionTreeGate gating:id="Q1.LR">
        <gating:rootNode gating:parameter="linToLog P3" gating:threshold="1.19140625">
            <gating:leafLT gating:inside="false"/>
             <gating:nodeGTE gating:parameter="linToLog P4" gating:threshold="1.3046874999999998">
                <gating:leafLT gating:inside="true"/>
                <gating:leafGTE gating:inside="false"/>
            </gating:nodeGTE>
        </gating:rootNode>
    </gating:DecisionTreeGate>
    
    <!-- G1, G2, G3 are just R1, R2, R3, respectively -->
    
    <gating:BooleanGate gating:id="R1-filtered-M1a">
        <gating:and>
            <gating:gateReference gating:ref="R1"/>
            <gating:gateReference gating:ref="M1a"/>
        </gating:and>
    </gating:BooleanGate>
    
    <gating:BooleanGate gating:id="R1-filtered-M1b">
        <gating:and>
            <gating:gateReference gating:ref="R1"/>
            <gating:gateReference gating:ref="M1b"/>
        </gating:and>
    </gating:BooleanGate>
    
    <gating:BooleanGate gating:id="R1-filtered-M2a">
        <gating:and>
            <gating:gateReference gating:ref="R1"/>
            <gating:gateReference gating:ref="M2a"/>
        </gating:and>
    </gating:BooleanGate>
    
    <gating:BooleanGate gating:id="R1-filtered-M2b">
        <gating:and>
            <gating:gateReference gating:ref="R1"/>
            <gating:gateReference gating:ref="M2b"/>
        </gating:and>
    </gating:BooleanGate>
    
    <gating:BooleanGate gating:id="R1-filtered-Q1.UL">
        <gating:and>
            <gating:gateReference gating:ref="R1"/>
            <gating:gateReference gating:ref="Q1.UL"/>
        </gating:and>
    </gating:BooleanGate>
    
    <gating:BooleanGate gating:id="R1-filtered-Q1.UR">
        <gating:and>
            <gating:gateReference gating:ref="R1"/>
            <gating:gateReference gating:ref="Q1.UR"/>
        </gating:and>
    </gating:BooleanGate>
    
    <gating:BooleanGate gating:id="R1-filtered-Q1.LL">
        <gating:and>
            <gating:gateReference gating:ref="R1"/>
            <gating:gateReference gating:ref="Q1.LL"/>
        </gating:and>
    </gating:BooleanGate>
    
    <gating:BooleanGate gating:id="R1-filtered-Q1.LR">
        <gating:and>
            <gating:gateReference gating:ref="R1"/>
            <gating:gateReference gating:ref="Q1.LR"/>
        </gating:and>
    </gating:BooleanGate>
        
</gating:Gating-ML>